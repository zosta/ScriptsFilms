<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="WpfScriptFilms.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6"/>
  </startup>
  <appSettings>
    <add key="Disque1" value="C:"/>
    <add key="Disque2" value=""/>
    <add key="Disque3" value=""/>
    <add key="Disque4" value=""/>
    <add key="Disque5" value=""/>
    <add key="log4net.Config" value="log4net.config"/>
    <add key="log4net.Config.Watch" value="True"/>
  </appSettings>
  <userSettings>
    <WpfScriptFilms.Properties.Settings>
      <setting name="Disque1" serializeAs="String">
        <value>F:\</value>
      </setting>
      <setting name="Disque2" serializeAs="String">
        <value />
      </setting>
      <setting name="Disque3" serializeAs="String">
        <value />
      </setting>
      <setting name="Disque4" serializeAs="String">
        <value />
      </setting>
      <setting name="Disque5" serializeAs="String">
        <value />
      </setting>
      <setting name="NomFichierExport" serializeAs="String">
        <value>MesFilms</value>
      </setting>
      <setting name="EmplacementFichierExport" serializeAs="String">
        <value>F:\</value>
      </setting>
    </WpfScriptFilms.Properties.Settings>
  </userSettings>
  <!--Conf log4Net-->
  <log4net>
    <!-- A1 is set to be a ConsoleAppender -->
    <appender name="A1" type="log4net.Appender.ConsoleAppender">

      <!-- A1 uses PatternLayout -->
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%-4timestamp [%thread] %-5level %logger %ndc - %message%newline" />
      </layout>
    </appender>

    <!-- Set root logger level to DEBUG and its only appender to A1 -->
    <root>
      <level value="DEBUG" />
      <appender-ref ref="A1" />
    </root>
  </log4net>
</configuration>

